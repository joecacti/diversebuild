<config>
<fields name="params">
<fieldset name="Basic" label="TEMPLATE CONFIGURATION ">
<field name="logo" type="media" label=" SELECT LOGO" default="templates/housedesign/images/logo.png" />
<field name="responsive" type="radio" default="yes" class="btn-group btn-group-yesno" label="RESPONSIVE">
<option value="yes"> YES</option>
<option value="no"> NO</option>
</field>
<field name="templatestyle" type="list" default="style1" label="PRESET STYLE">
<option value="style1"> STYLE 1</option>
<option value="style2"> STYLE 2</option>
<option value="style3"> STYLE 3</option>
<option value="style4"> STYLE 4</option>
<option value="style5"> STYLE 5</option>
</field>
<field name="fontfamily" type="list" default="fontfamily1" label="FONT FAMILY">
<option value="fontfamily1"> ARIAL</option>
<option value="fontfamily2"> TIMES NEW ROMAN</option>
<option value="fontfamily3"> COURIER NEW</option>
<option value="fontfamily4"> GEORGIA</option>
<option value="fontfamily5"> VERDANA</option>
<option value="fontfamily6"> GENEVA</option>
<option value="fontfamily7"> BOOK ANTIQUA</option>
</field>
<field name="centerposition" type="radio" default="centerright" class="btn-group btn-group-yesno" label="CENTER COLUMN">
<option value="centerleft"> LEFT</option>
<option value="centerright"> RIGHT</option>
</field>
<field name="expand" type="list" default="no" label=" WEBSITE WIDTH">
<option value="no"> 970PX</option>
<option value="yes"> 1060PX</option>
<option value="true"> 1240PX</option>
</field>
<field name="bannerwidth" type="radio" default="fix" class="btn-group btn-group-yesno" label=" BANNER WIDTH">
<option value="fix"> FIX</option>
<option value="wide"> WIDE</option>
</field>
<field name="autowide" type="radio" default="no" class="btn-group btn-group-yesno" label=" AUTO HIDE WIDE">
<option value="yes"> YES</option>
<option value="no"> NO</option>
</field>
<field name="copyright" type="text" default="" label=" COPYRIGHT" />
<field name="showdesigner" type="radio" default="yes" class="btn-group btn-group-yesno" label=" SHOW DESIGN BY">
<option value="yes"> YES</option>
<option value="no"> NO</option>
</field>
<field name="webdesigner" type="text" default="JoomlaSaver" label=" DESIGN BY TEXT" />
<field name="designlink" type="text" default="www.joomlasaver.com" label=" DESIGN BY LINK" />
</fieldset>
</fields>
