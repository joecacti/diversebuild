<files>
<filename module="mod_map">mod_map.php</filename>
<filename>index.html</filename>
<folder>tmpl</folder>
</files>
<config>
<fields name="params">
<fieldset name="basic">
<field name="map_latitude" type="text" default="" label="Latitude" />
<field name="map_longitude" type="text" default="" label="Longitude" />
<field name="map_zoom" type="text" default="16" label="Zoom" />
<field name="map_height" type="text" default="200" label="Height" />
<field name="map_class" type="text" default="" label="Class" />
<field type="spacer" />
<field name="map_address" type="text" default="" label="Address" />
<field name="map_phone" type="text" default="" label="Phone" />
<field name="map_email" type="text" default="" label="E Mail" />
</fieldset>
</fields>
</config>
</extension>